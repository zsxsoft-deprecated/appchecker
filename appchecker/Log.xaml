        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppChecker"
        mc:Ignorable="d"
        Title="Log" Height="307" Width="724">
    <Grid>
        <RichTextBox x:Name="richTextBox" Background="Black" Foreground="White" FontSize="16" FontFamily="Consolas" VerticalScrollBarVisibility="Visible">
            <FlowDocument>
                <Paragraph>
                    <Run Text="日志"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
